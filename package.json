{
  "name": "bscs-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"next dev\" \"tailwindcss --input ./src/global_styles/input.css --output ./src/global_styles/output.css --watch\"",
    "tdev": "concurrently \"next dev --turbo\" \"tailwindcss --input ./src/global_styles/input.css --output ./src/global_styles/output.css --watch\"",
    "build": "tailwindcss ./src/global_styles/input.css --output ./src/global_styles/output.css && next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@apollo/client": "^3.8.0-alpha.15",
    "@apollo/experimental-nextjs-app-support": "^0.1.0",
    "@headlessui/react": "^1.7.15",
    "@heroicons/react": "^2.0.18",
    "@tailwindcss/forms": "^0.5.3",
    "@types/node": "20.2.5",
    "@types/react": "18.2.7",
    "@types/react-dom": "18.2.4",
    "axios": "^1.4.0",
    "date-fns": "^2.30.0",
    "eslint": "8.41.0",
    "framer-motion": "^10.12.16",
    "graphql": "^16.6.0",
    "jsdom": "^22.1.0",
    "lottie-react": "^2.4.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-google-recaptcha": "^3.1.0",
    "react-hook-form": "^7.44.3",
    "react-number-format": "^5.2.2",
    "typescript": "5.0.4",
    "zustand": "^4.3.8"
  },
  "devDependencies": {
    "@types/jsdom": "^21.1.1",
    "@types/react-google-recaptcha": "^2.1.5",
    "autoprefixer": "^10.4.14",
    "concurrently": "^8.1.0",
    "daisyui": "^3.1.1",
    "eslint-config-next": "^13.4.5",
    "postcss": "^8.4.24",
    "tailwindcss": "^3.3.2"
  }
}
